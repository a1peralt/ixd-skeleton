	background-size: cover;
	background-repeat: no-repeat;">

	<div id="inactivator"></div>

			<div id ="hamburger">
			<img src="https://raw.githubusercontent.com/a1peralt/ixd-skeleton/master/resources/hamburger.png" height=50px width=50px>
			</div>

			<ul id= "cross">
				<img src="https://raw.githubusercontent.com/a1peralt/ixd-skeleton/master/resources/x.png" height=50px width=50px>
			</ul>
				
			<div class = "panel">

				<ul id ="hamWords" style="list-style:none; line-height:120px;">

                    <li>
                        <a href="/logged/home">
                        <img src="https://raw.githubusercontent.com/a1peralt/ixd-skeleton/master/resources/hamburger_icons/home.png"
                        class='ham-icons' style="left: -25%; top: 7.5%;">
                        Home
                        </a>
                    </li>

                    <li>
                        <a class ="myGarden">
                        <img src="https://raw.githubusercontent.com/a1peralt/ixd-skeleton/master/resources/hamburger_icons/plant.png"
                        class='ham-icons' style="left: -25%; top: 27.5%;">
                        My Garden
                        </a>
                    </li>
                    <!--
                    <li>
                        <a href="/logged/collaborate">
                        <img src="https://raw.githubusercontent.com/a1peralt/ixd-skeleton/master/resources/hamburger_icons/collaborate.png"
                        class='ham-icons' style="left: -25%; top: 47.5%;">
                        Collaborate
                        </a>
                    </li>
                    `-->
                    <li>
                        <a href="/logged/help">
                        <img src="https://raw.githubusercontent.com/a1peralt/ixd-skeleton/master/resources/hamburger_icons/question.png"
                        class='ham-icons' style="left: -25%; top: 47.5%;">
                        Help
                        </a>
                    </li>

                    <li>
                        <a href="/login">
                        <img src="https://raw.githubusercontent.com/a1peralt/ixd-skeleton/master/resources/hamburger_icons/logout.png"
                        class='ham-icons' style="left: -25%; top: 67.5%;">
                        Logout
                        </a>
                    </li>
                    
				</ul>
			</div>
				

    <div id=blockAlt>
            <div class = "vine-position">
        <img src = "https://scontent-lax3-2.xx.fbcdn.net/v/t1.15752-9/88064072_509684653024774_8994897630886625280_n.png?_nc_cat=107&_nc_sid=b96e70&_nc_ohc=sjIqA9n2aCgAX-RGwqo&_nc_ht=scontent-lax3-2.xx&oh=621a3812ccf123dd3b8c94bbd531c0ba&oe=5E97A64F" id = "vine">
        
    </div>
        <div class ="flower-position">
            <img src = "https://github.com/a1peralt/ixd-skeleton/blob/master/resources/flower.png?raw=true" id = "flower">
        </div>
    </div>

    <div id=MyGardenTitleAlt>
        my garden

        <div id=page-descriptionAlt> plants grown </div>

        <div id=circBGAlt> {{count}} </div>
    </div>


    <div id=plant-containerAlt>

        {{#each plants}}
        {{#if @first}}
        <div class=timeline style="display: none"></div>
        {{else}}
        <div class=timeline></div>
        {{/if}}
        <div class=plantAlt id={{@index}}>
            <img src={{image}} width="105px" height="105px">
            <div class=plantInfoAlt>
            <p id=name>{{name}}</p>
            <p id=date>{{date}}</p>
            <p id=time>{{time}}</p>
            </div>
        </div>
        {{/each}}
        
    </div>

</body>
</html>